/**
 * Creates result object serialized in a json response
 *
 * @input Order: dw.order.Order The found order.
 * @output Result: Object A result object.
 */

importPackage(dw.system);
importPackage(dw.order);

function execute(pdict : PipelineDictionary) : Number
{
    var order: Order = pdict.Order;

    pdict.Result = {
       orderID: order.getUUID()
    };

    return PIPELET_NEXT;
}