/**
*    This script returns journey order numbers for the given order
*
*    @input Order : dw.order.Order An order to fetch order numbers from.
*    @input Separator : String A separator.
*    @output OrderNumbers : String Order numbers.
*
*/
importPackage( dw.system );

importScript("checkout/Utils.ds");

function execute( args : PipelineDictionary ) : Number
{
   args.OrderNumbers = getOrderJourneysIds(args.Order, args.Separator);

   return PIPELET_NEXT;
}
