/**
* Demandware Script File
* To define input and output parameters, create entries of the form:
*
* @<paramUsageType> <paramName> : <paramDataType> [<paramComment>]
*
* where
*   <paramUsageType> can be either 'input' or 'output'
*   <paramName> can be any valid parameter name
*   <paramDataType> identifies the type of the parameter
*   <paramComment> is an optional comment
*
* For example:
*
*-   @input ExampleIn : String This is a sample comment.
*-   @output ExampleOut : Number
*
*/
importPackage( dw.system );
importPackage( dw.net );
importPackage( dw.util );

var logger : Logger = Logger.getLogger('sitemapreader');

function execute( args : PipelineDictionary ) : Number
{
    var request : HTTPClient = new HTTPClient();
    var requestString : String="http://dev10.store.converse.demandware.net/be/en/sitemap_0.xml";
    
    request.open("GET", requestString);
    request.send();
    
    var xmlObj : XML = new XML(request.getText());
    var xmlUrlList : XMLList = xmlObj.children().children();
	var resultlist : LinkedHashSet = new LinkedHashSet();
	
    for each( var itemurl in xmlUrlList)
    {
    	var xmlTagName : String = itemurl.name();
		resultlist.add( xmlTagName );
	 	logger.info(xmlTagName);  	
    }
    return PIPELET_NEXT;
}
