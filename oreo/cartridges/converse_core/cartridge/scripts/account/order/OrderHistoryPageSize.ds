/**
 * This script returns the PagingSize to use for searching Orders of a particular customer. It is calculated based on max capacity of multicountry sites.
 *
 * @output PageSize : Number the Size of the Page for the search.
 */
importPackage(dw.system);
importScript("converse_core:constants.ds");


function execute(args : PipelineDictionary) : Number
{
	
    args.PageSize = converse.constants.MaxOrderHistoryPageSizeForSearch;
    
    return PIPELET_NEXT;
}
