<iscontent type="application/json" charset="UTF-8" compact="true"/>
<isdecorate template="util/jsonresponse">
    <isscript>
        var o = { success:false };

        if (!empty(pdict.ErrorCode)) {
            o.error = pdict.ErrorCode;
        }
        
        if (!empty(pdict.FormErrors)) {
            o.formErrors = pdict.FormErrors;
        }
        
        var ojson = JSON.stringify(o);
    </isscript>
    <isprint value="${ojson}" encoding="off"/>
</isdecorate>
