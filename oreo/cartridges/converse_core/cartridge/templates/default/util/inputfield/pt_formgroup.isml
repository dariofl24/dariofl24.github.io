<isif condition="${formfield.label.visible}">
    <label for="${formfield.html.name}">
        <span>
            <isprint value="${Resource.msg(formfield.label.key,'forms',null)}"/>
        </span>
    </label>
</isif>
<isif condition="${formfield.showValue}">
    <isreplace/>
    <isinclude template="util/inputfield/message" />
</isif>