/**
* Get a feature by name
*
* @input FeatureName : String The feature name
* @output Feature : Object
*
*/
importPackage(dw.system);

importScript("converse_core:constants.ds");
importScript("converse_featuretoggle:lib/FeatureToggleService.ds");

function execute(args : PipelineDictionary) : Number
{
    var FeatureToggleService = converse.featuretoggle.FeatureToggleService;

    args.Feature = FeatureToggleService.getFeature(args.FeatureName);

    return PIPELET_NEXT;
}
