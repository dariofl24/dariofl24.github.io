/**
 * @input Message : String 
 * @output IsProfane : Boolean
 */
importPackage( dw.system );
importScript("converse_core:constants.ds");

importScript("int_nike:profane/ProfaneCheckService.ds");

function execute( args : PipelineDictionary ) : Number
{
    let profaneCheckService = new integration.nike.profanecheck.ProfaneCheckService();

    args.IsProfane = profaneCheckService.isProfane(args.Message);

    return PIPELET_NEXT;
}
