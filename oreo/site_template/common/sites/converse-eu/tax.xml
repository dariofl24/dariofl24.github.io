<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="eu-taxes">
            <display-name>EU Taxes</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="kids_tax">
            <display-name>kids tax class</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="regular_tax" default="true">
            <display-name>regular tax class</display-name>
            <description/>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="DK">
            <display-name>Denmark Tax Jurisdiction</display-name>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="EU Tax Jurisdiction" default="true">
            <display-name/>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="FI">
            <display-name>Finland Tax Jurisdiction</display-name>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="IE">
            <display-name>Ireland Tax Jurisdiction</display-name>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="DK" class-id="kids_tax">0.25</tax-rate>

        <tax-rate jurisdiction-id="EU Tax Jurisdiction" class-id="kids_tax">0</tax-rate>

        <tax-rate jurisdiction-id="FI" class-id="kids_tax">0.24</tax-rate>

        <tax-rate jurisdiction-id="IE" class-id="kids_tax">0</tax-rate>

        <tax-rate jurisdiction-id="DK" class-id="regular_tax">0.25</tax-rate>

        <tax-rate jurisdiction-id="EU Tax Jurisdiction" class-id="regular_tax">0</tax-rate>

        <tax-rate jurisdiction-id="FI" class-id="regular_tax">0.24</tax-rate>

        <tax-rate jurisdiction-id="IE" class-id="regular_tax">0.23</tax-rate>

        <tax-rate jurisdiction-id="DK" class-id="eu-taxes">0</tax-rate>

        <tax-rate jurisdiction-id="EU Tax Jurisdiction" class-id="eu-taxes">0</tax-rate>

        <tax-rate jurisdiction-id="FI" class-id="eu-taxes">0</tax-rate>

        <tax-rate jurisdiction-id="IE" class-id="eu-taxes">0</tax-rate>

    </tax-rates>
</tax>

