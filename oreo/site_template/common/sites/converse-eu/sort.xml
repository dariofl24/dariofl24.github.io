    <sorting-rule rule-id="Availability Ranking">
        <description>Pushes high inventory product to the top with a .35 threshold</description>
        <sorting-attributes>
            <category-position/>
            <availability-status/>
            <attribute>
                <attribute-path>product.custom.pillar</attribute-path>
                <direction>descending</direction>
                <include-text-relevance>false</include-text-relevance>
            </attribute>
        </sorting-attributes>
    </sorting-rule>

    <sorting-rule rule-id="Inventory Level high to top">
        <description>Use for Sale Sections to ensure items with higher inventory levels remain at the top</description>
        <sorting-attributes>
            <category-position/>
            <attribute>
                <attribute-path>product.custom.pillar</attribute-path>
                <direction>descending</direction>
                <include-text-relevance>false</include-text-relevance>
            </attribute>
            <attribute>
                <attribute-path>product.availabilityModel.ats</attribute-path>
                <direction>descending</direction>
                <include-text-relevance>false</include-text-relevance>
            </attribute>
            <attribute>
                <attribute-path>product.custom.brandSegment</attribute-path>
                <direction>ascending</direction>
                <include-text-relevance>false</include-text-relevance>
            </attribute>
        </sorting-attributes>
    </sorting-rule>

    <sorting-rule rule-id="by segment">
