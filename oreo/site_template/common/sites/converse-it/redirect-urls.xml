    <redirect-url uri="/en/home">
        <status-code>301</status-code>
        <enabled-flag>true</enabled-flag>
        <destination-url>/it/en/go</destination-url>
        <copy-source-params>yes</copy-source-params>
    </redirect-url>

    <redirect-url uri="/it/go">
        <status-code>301</status-code>
        <enabled-flag>true</enabled-flag>
        <destination-url>/it</destination-url>
        <copy-source-params>yes</copy-source-params>
    </redirect-url>

    <redirect-url uri="/it/home">
        <status-code>301</status-code>
        <enabled-flag>true</enabled-flag>
        <destination-url>/it</destination-url>
        <copy-source-params>yes</copy-source-params>
    </redirect-url>

    <redirect-url uri="/giftcards">
        <status-code>301</status-code>
        <enabled-flag>true</enabled-flag>
        <destination-url>/</destination-url>
        <copy-source-params>no</copy-source-params>
