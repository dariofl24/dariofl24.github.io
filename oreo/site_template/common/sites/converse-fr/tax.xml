<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="Standard" default="true">
            <display-name>Standard</display-name>
            <description>Default Tax Class</description>
        </tax-class>

        <tax-class class-id="kids_tax">
            <display-name>kids tax class</display-name>
            <description/>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="FR" default="true">
            <display-name>FR Tax Jurisdiction</display-name>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="LU">
            <display-name>LU Jurisdiction</display-name>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="LU" class-id="kids_tax">0.03</tax-rate>

        <tax-rate jurisdiction-id="FR" class-id="kids_tax">0.2</tax-rate>

        <tax-rate jurisdiction-id="LU" class-id="Standard">0.17</tax-rate>

        <tax-rate jurisdiction-id="FR" class-id="Standard">0.2</tax-rate>

    </tax-rates>
</tax>
