<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="Standard" default="true">
            <display-name>Default Tax</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="kids_tax">
            <display-name>kids tax class</display-name>
            <description/>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="ES">
            <display-name>ES Tax Jurisdiction</display-name>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="PT" default="true">
            <display-name>PT Tax Jurisdiction</display-name>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="ES" class-id="kids_tax">0.21</tax-rate>

        <tax-rate jurisdiction-id="PT" class-id="kids_tax">0.23</tax-rate>

        <tax-rate jurisdiction-id="ES" class-id="Standard">0.21</tax-rate>

        <tax-rate jurisdiction-id="PT" class-id="Standard">0.23</tax-rate>
    </tax-rates>
</tax>
