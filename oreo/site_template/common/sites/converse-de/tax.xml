<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="Standard" default="true">
            <display-name>Default Tax</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="kids_tax">
            <display-name>kids tax class</display-name>
            <description/>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="AT">
            <display-name>AT Tax Jurisdiction</display-name>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="DE" default="true">
            <display-name>DE Tax Jurisdiction</display-name>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="DE" class-id="kids_tax">0.19</tax-rate>

        <tax-rate jurisdiction-id="AT" class-id="kids_tax">0.2</tax-rate>

        <tax-rate jurisdiction-id="DE" class-id="Standard">0.19</tax-rate>

        <tax-rate jurisdiction-id="AT" class-id="Standard">0.2</tax-rate>

    </tax-rates>
</tax>
