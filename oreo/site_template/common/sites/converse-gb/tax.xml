<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="UK Exempt">
            <display-name>0 Rate</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="gb-taxes" default="true">
            <display-name>GB Taxes</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="kids_tax">
            <display-name>kids tax class</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="tax-exempt">
            <display-name>Tax Exempt</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="uk-vat">
            <display-name>UK VAT</display-name>
            <description/>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="GB Tax Jurisdiction" default="true">
            <display-name/>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="UK VAT Jurisdiction">
            <display-name/>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="UK VAT Jurisdiction" class-id="kids_tax">0</tax-rate>

        <tax-rate jurisdiction-id="GB Tax Jurisdiction" class-id="kids_tax">0</tax-rate>

        <tax-rate jurisdiction-id="UK VAT Jurisdiction" class-id="UK Exempt">0</tax-rate>

        <tax-rate jurisdiction-id="GB Tax Jurisdiction" class-id="UK Exempt">0</tax-rate>

        <tax-rate jurisdiction-id="UK VAT Jurisdiction" class-id="uk-vat">0.2</tax-rate>

        <tax-rate jurisdiction-id="GB Tax Jurisdiction" class-id="uk-vat">0</tax-rate>

        <tax-rate jurisdiction-id="UK VAT Jurisdiction" class-id="gb-taxes">0</tax-rate>

        <tax-rate jurisdiction-id="GB Tax Jurisdiction" class-id="gb-taxes">0.2</tax-rate>

        <tax-rate jurisdiction-id="UK VAT Jurisdiction" class-id="tax-exempt">0</tax-rate>

        <tax-rate jurisdiction-id="GB Tax Jurisdiction" class-id="tax-exempt">0</tax-rate>

    </tax-rates>
</tax>
