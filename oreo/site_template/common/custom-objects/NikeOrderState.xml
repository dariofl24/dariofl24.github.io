<?xml version="1.0" encoding="UTF-8"?>
<custom-objects xmlns="http://www.demandware.com/xml/impex/customobject/2006-10-31">
    <!-- The custom object representing Nike Order State. Attribute "name" is used as a key mapped to localized display values in dyo.properties file -->
    <custom-object type-id="NikeOrderState" object-id="0">
        <object-attribute attribute-id="description">In Process (actual display values are mapped to the name and read from dyo.properties file)</object-attribute>
        <object-attribute attribute-id="name">UNKNOWN</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="1">
        <object-attribute attribute-id="description">In Process</object-attribute>
        <object-attribute attribute-id="name">INITIAL</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="2">
        <object-attribute attribute-id="description">In Process</object-attribute>
        <object-attribute attribute-id="name">ORDER_RECEIVED</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="3">
        <object-attribute attribute-id="description">In Process</object-attribute>
        <object-attribute attribute-id="name">ORDER_PROCESSED</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="4">
        <object-attribute attribute-id="description">Order in Production</object-attribute>
        <object-attribute attribute-id="name">FACTORY_PROCESSING</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="5">
        <object-attribute attribute-id="description">Order Delayed</object-attribute>
        <object-attribute attribute-id="name">FACTORY_DELAYED</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="6">
        <object-attribute attribute-id="description">Order Completed at Factory. Preparing for Shipment.</object-attribute>
        <object-attribute attribute-id="name">FACTORY_COMPLETE</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="7">
        <object-attribute attribute-id="description">Preparing for shipment</object-attribute>
        <object-attribute attribute-id="name">MANIFEST_SCAN</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="8">
        <object-attribute attribute-id="description">Order has shipped and is being cleared through customs</object-attribute>
        <object-attribute attribute-id="name">ORIGIN_SCAN</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="9">
        <object-attribute attribute-id="description">Order has cleared customs and is in transit</object-attribute>
        <object-attribute attribute-id="name">SHIPPER_CLEARED</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="10">
        <object-attribute attribute-id="description">Delivered to a store</object-attribute>
        <object-attribute attribute-id="name">DELIVERED_STORE</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="11">
        <object-attribute attribute-id="description">Ready for pick up in a store</object-attribute>
        <object-attribute attribute-id="name">IN_STORE_PICKUP</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="12">
        <object-attribute attribute-id="description">Delivery Attempted</object-attribute>
        <object-attribute attribute-id="name">FIRST_DELIVERY</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="13">
        <object-attribute attribute-id="description">Unable to Deliver</object-attribute>
        <object-attribute attribute-id="name">DELIVER_FAILED</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="14">
        <object-attribute attribute-id="description">Order Delivered</object-attribute>
        <object-attribute attribute-id="name">DELIVERED</object-attribute>
    </custom-object>

    <custom-object type-id="NikeOrderState" object-id="15">
        <object-attribute attribute-id="description">Order Cancelled</object-attribute>
        <object-attribute attribute-id="name">CANCELLED</object-attribute>
    </custom-object>

</custom-objects>
