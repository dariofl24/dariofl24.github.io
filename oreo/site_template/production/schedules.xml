            <site site-id="converse-gb"/>
            <site site-id="converse-it"/>
            <site site-id="converse"/>
            <site site-id="converse-be"/>
            <site site-id="converse-eu"/>
        </sites>
        <recurrence>
            <date-from>2013-09-06Z</date-from>
            <start-time>16:42:00.000Z</start-time>
            <interval>60m</interval>
            <day-of-week>
                <weekday>Sunday</weekday>
                <weekday>Monday</weekday>
                <weekday>Tuesday</weekday>
                <weekday>Wednesday</weekday>
                <weekday>Thursday</weekday>
                <weekday>Friday</weekday>
                <weekday>Saturday</weekday>
            </day-of-week>
        </recurrence>
        <notification>
            <enabled-flag>true</enabled-flag>
            <email-from>confoo-reports-jobs@tacitknowledge.com</email-from>
            <email-to>confoo-orderguard@tacitknowledge.com</email-to>
            <email-to-cc>mkennetgillman@gmail.com,Lst-Converse.DigitalGorillaz@nike.com,chris.langway@converse.com,Dan.Ronner-Bland@converse.com,Cindy.Ascolillo@converse.com</email-to-cc>
            <threshold>
                <exception/>
                <error/>
                <hang/>
                <retry/>
            </threshold>
            <max-execution-time>20</max-execution-time>
            <hang-detection-enabled-flag>true</hang-detection-enabled-flag>
            <failure-rule>retry</failure-rule>
            <retries>3</retries>
            <retry-interval>10</retry-interval>
        </notification>
        <parameters>
            <parameter name="NotifyTo">confoo-reports-jobs@tacitknowledge.com</parameter>
            <parameter name="MaxAge">60</parameter>
        </parameters>
    </schedule>

