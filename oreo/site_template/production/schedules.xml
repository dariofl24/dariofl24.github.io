    <schedule schedule-id="ArvatoOrderExport">
        <description/>
        <enabled-flag>true</enabled-flag>
        <priority>0</priority>
        <execution-scope>site</execution-scope>
        <pipeline>
            <name>OrderService</name>
            <startnode>Export</startnode>
        </pipeline>
        <sites>
            <site site-id="converse-fr"/>
            <site site-id="converse-es"/>
            <site site-id="converse-de"/>
            <site site-id="converse-nl"/>
            <site site-id="converse-gb"/>
            <site site-id="converse-it"/>
            <site site-id="converse-be"/>
            <site site-id="converse-eu"/>
        </sites>
        <recurrence>
            <date-from>2015-10-26Z</date-from>
            <start-time>06:46:00.000Z</start-time>
            <interval>15m</interval>
            <day-of-week>
                <weekday>Sunday</weekday>
                <weekday>Monday</weekday>
                <weekday>Tuesday</weekday>
                <weekday>Wednesday</weekday>
                <weekday>Thursday</weekday>
                <weekday>Friday</weekday>
                <weekday>Saturday</weekday>
            </day-of-week>
        </recurrence>
        <notification>
            <enabled-flag>false</enabled-flag>
            <hang-detection-enabled-flag>false</hang-detection-enabled-flag>
        </notification>
    </schedule>

    <schedule schedule-id="ExportCustomers">
        <description>Exports all customers, on demand.</description>
        <enabled-flag>true</enabled-flag>
        <priority>0</priority>
        <execution-scope>site</execution-scope>
        <pipeline>
            <name>ExportCustomers</name>
            <startnode>Start</startnode>
        </pipeline>
        <sites>
            <site site-id="converse"/>
        </sites>
        <run-once>2016-01-28T22:08:00.000Z</run-once>
        <notification>
            <enabled-flag>true</enabled-flag>
            <email-from>confoo-reports-jobs@tacitknowledge.com</email-from>
            <email-to>confoo-reports-jobs@tacitknowledge.com, cindy.ascolillo@converse.com</email-to>
            <threshold>
                <success/>
                <exception/>
                <error/>
                <hang/>
                <retry/>
            </threshold>
            <max-execution-time>60</max-execution-time>
            <hang-detection-enabled-flag>false</hang-detection-enabled-flag>
            <failure-rule>retry</failure-rule>
            <retries>3</retries>
            <retry-interval>10</retry-interval>
        </notification>
    </schedule>

    <schedule schedule-id="IntegrationFrameworkCleanupJob">
        <description>This is the cleanup job which removes old custom objects created by the integration framework&#13;
@input workflowMaximumObjectAgeBeforeDeletion : String As job parameter. The maximum age of the custom objects.</description>
        <enabled-flag>false</enabled-flag>
        <priority>0</priority>
        <execution-scope>organization</execution-scope>
        <pipeline>
            <name>Workflow</name>
            <startnode>CleanUp</startnode>
        </pipeline>
        <run-once>2015-10-26T06:47:00.000Z</run-once>
        <notification>
            <enabled-flag>false</enabled-flag>
            <hang-detection-enabled-flag>false</hang-detection-enabled-flag>
        </notification>
    </schedule>

    <schedule schedule-id="OrderGuard">
        <description/>
        <enabled-flag>true</enabled-flag>
        <priority>0</priority>
        <execution-scope>site</execution-scope>
        <pipeline>
            <name>OrderGuard</name>
            <startnode>Run</startnode>
        </pipeline>
        <sites>
            <site site-id="converse-fr"/>
            <site site-id="converse-es"/>
